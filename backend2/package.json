        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "ts-jest": "^28.0.2",
        "ts-node": "^10.8.0",
        "typescript": "~4.6.4"
    },
    "dependencies": {
        "@aws-cdk/aws-cognito": "^1.157.0",
        "@aws-cdk/core": "^1.157.0",
        "@slack/bolt": "^3.11.1",
        "aws-cdk-lib": "2.25.0",
        "aws-lambda": "^1.0.7",
